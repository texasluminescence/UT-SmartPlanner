{
    "manifest_version": 3,
    "name": "UT Smart Planner", 
    "icons": {
        "32": "icons/icon32.png"
    },
    "action": {
        "default_popup": "courses.html",
        "default_icon": "icons/icon32.png"

    },
    "version": "1.0.0",
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "host_permissions": [
    "http://localhost:5000/"
    ],
    "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
    }
    ]
}